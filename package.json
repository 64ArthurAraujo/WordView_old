{
  "name": "wordview",
  "description": "Learn by visualizing words in their rawest form! ",
  "version": "0.1.6",
  "author": {
    "name": "64ArthurAraujo"
  },
  "main": "./src/electron.js",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -wl",
    "build-electron": "run-s build start-electron",
    "start-electron": "electron ."
  },
  "build": {
    "appId": "io.wordview",
    "mac": {
      "category": "io.wordview"
    },
    "win": {
      "publisherName": "Arthur Araujo",
      "requestedExecutionLevel": "asInvoker",
      "target": [
        "portable", "nsis"
      ]
    },
    "linux": {
      "synopsis": "Learn by visualizing words in their rawest form!",
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/node": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "electron-builder": "^23.3.3"
  },
  "dependencies": {
    "feather-icons": "^4.29.0",
    "sirv-cli": "^2.0.0",
    "svelte-navigator": "^3.1.6"
  }
}
